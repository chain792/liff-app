<h1>トップページ</h1>
<% if logged_in? %>
  <%= link_to "ログアウト", logout_path, method: :delete%>
<% else %>
  <%= link_to "ログイン", login_path %>
<% end %>

<% if logged_in? %>
  <%= form_with(model: PhoneNumberForm.new, url: push_path, local: true) do |f|  %>
    <%= f.label :number, "電話番号"%>
    <%= f.text_field :number %>
    <%= f.submit '送信' %>
  <% end %>
<% end %>